{
  "name": "trendguesser",
  "description": "A game where you guess the trend of search terms over time.",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "dev:plain": "next dev",
    "build": "next build",
    "prestart": "npm run db:init:standalone",
    "start": "next dev",
    "lint": "next lint",
    "deploy": "./scripts/deploy-vercel.sh",
    "clean": "rm -rf .next node_modules out",
    "db:init": "node scripts/init-db.js",
    "db:init:prod": "node scripts/init-db-simple.js",
    "db:init:standalone": "node src/scripts/standalone-init.js",
    "db:check": "node src/scripts/check-db-connection.js",
    "db:setup": "./scripts/setup-database.sh",
    "db:init:api": "curl -X POST http://localhost:3000/api/init-db-now",
    "db:reset": "curl -X POST \"http://localhost:3000/api/init-db-now?force=true\"",
    "convert-csv": "node src/scripts/csv-to-json.js",
    "predev": "npm run convert-csv"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^0.8.1",
    "@next/bundle-analyzer": "^15.2.4",
    "@tailwindcss/postcss": "^4.1.1",
    "@types/react": "^19.1.2",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.21",
    "framer-motion": "^12.6.3",
    "next": "15.2.4",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "react": "^19.0.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^19.0.0",
    "seedrandom": "^3.0.5",
    "swr": "^2.3.3",
    "tailwindcss": "^4.1.1",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.0"
  }
}
